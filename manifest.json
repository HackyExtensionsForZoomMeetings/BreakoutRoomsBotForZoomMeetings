{
  "name": "Hacky The Bot for Zoom Meetings",
  "version": "1.0",
  "description": "Hacky the Bot will help you with Zoom Meetings Breakout Rooms",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "BreakoutRoomBot.js"
  ],
  "manifest_version": 2
}
